{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "React/Redux Boilerplate",
  "repository": "https://github.com/Alemkin/react-redux-boilerplate",
  "main": "/app",
  "author": "Alexander Lemkin",
  "scripts": {
    "lint": "standard \"./app/**/*.js\" | snazzy",
    "test": "cross-env NODE_ENV=test jest --verbose --watchAll",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --disable-host-check",
    "story": "start-storybook -p 9998",
    "build-storybook": "build-storybook"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testRegex": "\\.test\\.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "async-ops": "1.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "date-fns": "1.30.1",
    "i18next": "13.0.0",
    "lodash": "4.17.11",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "reactstrap": "7.1.0",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "redux-form": "8.1.0",
    "redux-saga": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-actions": "4.1.2",
    "@storybook/addon-knobs": "4.1.2",
    "@storybook/addon-links": "4.1.2",
    "@storybook/addons": "4.1.2",
    "@storybook/react": "4.1.2",
    "addon-redux": "1.0.0",
    "autoprefixer": "9.4.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "bootstrap": "4.2.1",
    "circular-dependency-plugin": "5.0.2",
    "cross-env": "5.2.0",
    "css-loader": "2.0.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "i18next-resource-store-loader": "0.1.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "react-testing-library": "5.4.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "sass-loader": "7.1.0",
    "snazzy": "8.0.0",
    "standard": "12.0.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14",
    "webpack-encoding-plugin": "0.2.1",
    "webpack-node-externals": "1.7.2"
  }
}
