{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "React/Redux Boilerplate",
  "repository": "https://github.com/Alemkin/react-redux-boilerplate",
  "main": "/app",
  "author": "Alexander Lemkin",
  "scripts": {
    "lint": "eslint \"./app/**/*.js\"",
    "lint-fix": "eslint \"./app/**/*.js\" --fix",
    "lint-style": "stylelint \"app/**/*.scss\"",
    "lint-style-fix": "stylelint \"app/**/*.scss\" --fix",
    "test": "cross-env NODE_ENV=test jest --verbose --watchAll",
    "test-ci": "cross-env NODE_ENV=test jest --verbose",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --disable-host-check",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "eslintIgnore": [
    "*.test.js"
  ],
  "jest": {
    "testRegex": "\\.test\\.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@babel).+\\.js"
    ],
    "setupFiles": [
      "<rootDir>/test.config.js",
      "jest-canvas-mock"
    ]
  },
  "dependencies": {
    "async-operations": "1.1.9",
    "date-fns": "1.30.1",
    "i18next": "17.0.6",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "reactstrap": "8.5.1",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/polyfill": "7.10.4",
    "@babel/runtime": "7.10.4",
    "autoprefixer": "9.8.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "bootstrap": "4.3.1",
    "circular-dependency-plugin": "5.0.2",
    "cross-env": "7.0.2",
    "css-loader": "3.6.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.7.2",
    "eslint-config-standard": "14.1.1",
    "eslint-config-standard-jsx": "8.1.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "4.1.0",
    "html-webpack-plugin": "3.2.0",
    "i18next-resource-store-loader": "0.1.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.1.0",
    "jest-canvas-mock": "2.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "react-styleguidist": "11.0.8",
    "react-testing-library": "5.4.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "sass-loader": "7.1.0",
    "snazzy": "8.0.0",
    "standard": "14.3.4",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "18.3.0",
    "terser-webpack-plugin": "2.3.2",
    "url-loader": "1.1.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-encoding-plugin": "0.3.1",
    "webpack-node-externals": "1.7.2"
  }
}
